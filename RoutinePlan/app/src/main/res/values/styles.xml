<resources>
  <color name="redColorAccent">#ffff0046</color>
  <color name="redColorPrimaryDark">#ff9c0029</color>
  <color name="redColorPrimary">#d80039</color>

  <!-- Base application theme. -->
  <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/redColorPrimary</item>
    <item name="colorPrimaryDark">@color/redColorPrimaryDark</item>
    <item name="colorAccent">@color/redColorAccent</item>
  </style>

  <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
    <item name="android:windowBackground">@drawable/background_splash</item>
  </style>



  <!--Green Colors-->
  <color name="green">#4CAF50</color>
  <color name="greenDark">#388E3C</color>

  <style name="AppTheme.GreenTheme" parent="AppTheme">
    <item name="colorPrimary">@color/green</item>
    <item name="colorPrimaryDark">@color/greenDark</item>
    <item name="colorAccent">@color/green</item>
    <item name="actionModeBackground">@color/green</item>
    <item name="android:alertDialogTheme">@style/AppTheme.GreenTheme.Dialog</item>
  </style>

  <style name="AppTheme.GreenTheme.Dialog" parent="Theme.AppCompat.Light.Dialog">
    <item name="android:windowMinWidthMajor">@android:dimen/dialog_min_width_major</item>
    <item name="android:windowMinWidthMinor">@android:dimen/dialog_min_width_minor</item>
    <item name="colorAccent">@color/green</item>
  </style>

